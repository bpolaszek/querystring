{
    "name": "bentools/querystring",
    "description": "Query String manipulation library. PHP 7.1+. No dependency, immutable, PSR-7 compliant.",
    "keywords": ["url", "uri", "querystring", "query string", "psr-7", "psr7"],
    "authors": [
        {
            "name": "Beno!t POLASZEK",
            "email": "bpolaszek@gmail.com",
            "homepage": "http://www.polaszek.fr",
            "role": "Developer"
        }
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "league/uri": "^5.0",
        "php-coveralls/php-coveralls": "^2.4",
        "phpunit/phpunit": "^5.0|^6.0|^7.0",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/var-dumper": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "BenTools\\QueryString\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BenTools\\QueryString\\Tests\\": "tests"
        },
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
